# cubrid Database Settings
jdbc.driver = cubrid.jdbc.driver.CUBRIDDriver
cubrid.url = jdbc:cubrid:<%= link('cubrid_link').instances[0].address%>::<%= p('cubrid_broker.cubrid_db_name')%>:::
cubrid.portNumber = <%= p('cubrid_broker.cubrid_db_port')%>
cubrid.userName = <%= p('cubrid_broker.cubrid_db_user')%>
cubrid.password = <%= p('cubrid_broker.cubrid_db_passwd') %>

cubrid.server.hostName = <%= link('cubrid_link').instances[0].address%>
cubrid.server.portNumber = <%= p('cubrid_broker.cubrid_ssh_port')%>
cubrid.server.userName = <%= p('cubrid_broker.cubrid_ssh_user')%>
# password or identify Only one of the two
cubrid.server.password =  
cubrid.server.identity = /var/vcap/jobs/cubrid_broker/ssh/private_key.pem
cubrid.server.sudo.password =  
